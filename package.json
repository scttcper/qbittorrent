{
  "name": "qbittorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for qbittorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/qbittorrent",
  "main": "./index.js",
  "typings": "./index.d.ts",
  "sideEffects": false,
  "keywords": [
    "typescript",
    "qbittorrent"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prebuild": "rimraf dist",
    "build": "tsc -p tsconfig.json",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "postbuild": "cp package.json dist && cp LICENSE dist && cp README.md dist",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --ci --runInBand --reporters=default --reporters=jest-junit --coverage",
    "semantic-release": "cd dist && semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.1.2",
    "form-data": "^2.3.3",
    "got": "^9.6.0",
    "tough-cookie": "^3.0.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.4.1",
    "@types/jest": "24.0.11",
    "@types/node": "11.13.4",
    "@types/p-wait-for": "3.0.1",
    "@types/tough-cookie": "2.3.5",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "1.6.0",
    "@typescript-eslint/parser": "1.6.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.1.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.9.0",
    "eslint-plugin-import": "2.17.1",
    "jest": "24.7.1",
    "jest-junit": "6.3.0",
    "p-wait-for": "3.1.0",
    "rimraf": "2.6.3",
    "semantic-release": "15.13.3",
    "ts-jest": "24.0.2",
    "typedoc": "0.14.2",
    "typescript": "3.4.3"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
