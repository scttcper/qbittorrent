{
  "name": "@ctrl/qbittorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for qbittorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "typectrl/qbittorrent",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "qbittorrent"
  ],
  "scripts": {
    "lint": "eslint src/**/*.ts test/**/*.ts",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "prepare": "npm run build",
    "build": "del dist && tsc",
    "build:docs": "typedoc --out docs --target es6 --theme minimal --mode file src && touch docs/.nojekyll",
    "test": "jest --runInBand",
    "test:watch": "jest --watch --runInBand",
    "test:ci": "jest --ci --runInBand --coverage",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "@ctrl/shared-torrent": "^1.3.1",
    "form-data": "^2.3.3",
    "got": "^9.6.0",
    "parse-torrent": "^6.1.2",
    "tough-cookie": "^3.0.1",
    "url-join": "^4.0.0"
  },
  "devDependencies": {
    "@types/form-data": "2.2.1",
    "@types/got": "9.4.4",
    "@types/jest": "24.0.12",
    "@types/node": "12.0.0",
    "@types/parse-torrent": "5.8.3",
    "@types/tough-cookie": "2.3.5",
    "@types/url-join": "4.0.0",
    "@typescript-eslint/eslint-plugin": "1.7.0",
    "@typescript-eslint/parser": "1.7.0",
    "del-cli": "1.1.0",
    "eslint": "5.16.0",
    "eslint-config-prettier": "4.2.0",
    "eslint-config-xo-space": "0.21.0",
    "eslint-config-xo-typescript": "0.10.1",
    "eslint-plugin-import": "2.17.2",
    "jest": "24.8.0",
    "p-wait-for": "3.1.0",
    "semantic-release": "15.13.12",
    "ts-jest": "24.0.2",
    "typedoc": "0.14.2",
    "typescript": "3.4.5"
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=8"
  }
}
