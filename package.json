{
  "name": "@ctrl/qbittorrent",
  "version": "0.0.0-placeholder",
  "description": "TypeScript api wrapper for qbittorrent using got",
  "author": "Scott Cooper <scttcper@gmail.com>",
  "license": "MIT",
  "repository": "scttcper/qbittorrent",
  "homepage": "https://qbittorrent.vercel.app",
  "type": "module",
  "main": "./dist/src/index.js",
  "typings": "./dist/src/index.d.ts",
  "files": [
    "dist/src"
  ],
  "sideEffects": false,
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "typescript",
    "qbittorrent"
  ],
  "scripts": {
    "lint": "eslint --ext .js,.ts, .",
    "lint:fix": "eslint --fix --ext .js,.ts, .",
    "prepare": "npm run build",
    "build": "tsc",
    "build:docs": "typedoc",
    "test": "ava",
    "test:watch": "ava --watch",
    "test:ci": "c8 --reporter=text --reporter=lcov ava"
  },
  "dependencies": {
    "@ctrl/magnet-link": "^3.1.0",
    "@ctrl/shared-torrent": "^4.1.0",
    "@ctrl/torrent-file": "^2.0.1",
    "@ctrl/url-join": "^2.0.0",
    "formdata-node": "^4.3.2",
    "got": "^12.0.1",
    "tough-cookie": "^4.0.0"
  },
  "devDependencies": {
    "@ctrl/eslint-config": "3.3.1",
    "@sindresorhus/tsconfig": "2.0.0",
    "@types/node": "17.0.21",
    "@types/tough-cookie": "4.0.1",
    "ava": "4.0.1",
    "c8": "7.11.0",
    "p-wait-for": "4.1.0",
    "ts-node": "10.5.0",
    "typedoc": "0.22.12",
    "typescript": "4.5.5"
  },
  "ava": {
    "files": [
      "test/**/*.spec.ts"
    ],
    "extensions": {
      "ts": "module"
    },
    "nodeArguments": [
      "--loader=ts-node/esm"
    ]
  },
  "release": {
    "branch": "master"
  },
  "engines": {
    "node": ">=14.16"
  }
}
